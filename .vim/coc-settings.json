{
	"pairs.enableCharacters": [
		"(",
		"{",
		"[",
		"'",
		"`"
	],
	"suggest.minTriggerInputLength": 2,
	"suggest.snippetIndicator": "â–º",
	"suggest.maxCompleteItemCount": 10,
	"suggest.labelMaxLength": 40,
	"suggest.noselect": false,
	"suggest.enablePreview": true,
	"suggest.removeDuplicateItems": true,
	"coc.preferences.currentFunctionSymbolAutoUpdate": true,
	"diagnostic.warningSign": "!",
	"diagnostic.checkCurrentLine": false,
	"coc.preferences.jumpCommand": "drop",
	"coc.preferences.rootPatterns": [
		".git",
		".vscode"
	],
	// codelens
	"codeLens.enable": true,
	"javascript.referencesCodeLens.enable": true,
	"java.completion.enabled": true, "coc.preferences.colorSupport": { 
	  "type": "boolean", 
	  "description": "Enable color highlight if language server support it.", 
	  "default": true 
	}, 
	// autoformat
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"java",
		"javascript",
		"javascriptreact",
		"json",
		"html",
		"markdown",
		"python",
		"tex",
		"typescript",
		"typescriptreact",
		"vim", 
		"rust"
	],
	// React Config
	"tsserver.formarOnType": true, 
	"coc.preferences.formarOnType": true,
	// Python Configs
	"jedi.executable.command": "jedi-language-server",
	"jedi.enable": true,
	"jedi.markupKindPreferred": "plaintext",
	"jedi.trace.server": "messages",
	"jedi.jediSettings.autoImportModules": [],
	"jedi.executable.args": [],
	"jedi.completion.disableSnippets": false,
	"python.formatting.provider": "autopep8",
	"jedi.diagnostics.enable": true,
	"jedi.diagnostics.didChange": true,
	"jedi.diagnostics.didSave": true,
	"python.linting.enabled": true,
	"python.linting.pylintEnabled": true,
	"python.jediEnabled": false,
	"python.pythonPath": "python",
	// Java Configs
	"java.referencesCodeLens.enabled": true,
	"java.jdt.ls.vmargs": "-javaagent:/usr/local/share/lombok/lombok.jar",
	// Javascript Configs
	"tsserver.enable": true,
	"tsserver.enableJavascript": true,
	// Markdown Configs
	"[markdown]": {
		"editor.quickSuggestions": {
			"other": true,
			"comments": true,
			"strings": true
		},
